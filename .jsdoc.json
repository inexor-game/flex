{
    "opts": {
        "destination": "./doc/",
        "encoding": "utf8",
        "recurse": true,
        "template": "node_modules/tui-jsdoc-template",
        "readme": "README.md"
    },
    "tags": {
        "allowUnknownTags": true,
        "dictionaries": ["jsdoc","closure"]
    },
    "source": {
        "include": ["server/", "src"],
        "includePattern": ".+\\.js(doc|x)?$",
        "excludePattern": "node_modules"
    },
    "templates": {
        "name": "Inexor Flex",
        "logo": {
            "url": "https://raw.githubusercontent.com/inexorgame/artwork/master/logo/logo_rendered.png",
            "width": "120px",
            "height": "80px"
        },
        "cleverLinks": false,
        "monospaceLinks": false
    }
}
